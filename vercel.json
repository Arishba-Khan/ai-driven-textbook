{
  "buildCommand": "npm run build:all",
  "outputDirectory": "build",
  "framework": "docusaurus",
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/auth-backend/src/server.ts"
    }
  ],
  "functions": {
    "auth-backend/src/server.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}